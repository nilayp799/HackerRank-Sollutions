# Complete the divisibleSumPairs function below.
def divisibleSumPairs(n, k, ar):
    arr=ar
    c=0
    for i in range(0,len(ar)):
        for j in range(i+1,len(arr)): #at this step we've started loop from i+1 so i is allways lessthan j
            if((ar[i]+arr[j])%k==0):
                c +=1

    return c
  
